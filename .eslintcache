nder={() => <SignUp />} />\n          <Route exact path=\"/board\" render={() => <Board />} />\n          <Route exact path=\"/map\" render={() => <Map />} />\n          <Route exact path=\"/petregister\" render={() => <PetRegister />} />\n        </Switch>\n      </div>\n    );\n  }\n}\n\nexport default withRouter(App);\n\n// function App() {\n//   return (\n//     <div>\n//       <Nav></Nav>\n//       <Main></Main>\n//       {/* <Board></Board> */}\n//       {/*  <Map /> */}\n//       {/* <PetRegister></PetRegister> */}\n//     </div>\n//   );\n// }\n\n// export default App;\n","/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/Main.jsx",["75"],"import React from \"react\";\nimport \"../styles/Main.css\";\nimport { Link, withRouter } from \"react-router-dom\";\n\n// import Signin from \"./Signin\";\n\nclass Main extends React.Component {\n  constructor(props) {\n    super(props);\n    // this.state = {\n    //   isModalOpen: false,\n    // };\n  }\n\n  // openModal = () => {\n  //   this.setState({ isModalOpen: true });\n  // };\n\n  // closeModal = () => {\n  //   this.setState({ isModalOpen: false });\n  // };\n\n  render() {\n    return (\n      <div className=\"flex\">\n        <div className=\"main\"></div>\n        <div className=\"pageList\">\n          <div className=\"map\">\n            <Link to=\"/map\">\n              <div className=\"ListBtn\">지도보기</div>\n            </Link>\n          </div>\n          <div className=\"board\">\n            <Link to=\"/board\">\n              <div className=\"ListBtn\">게시판</div>\n            </Link>\n          </div>\n          <div className=\"petRegister\">\n            <Link to=\"/petregister\">\n              <div className=\"ListBtn\">펫 등록하기</div>\n            </Link>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default withRouter(Main);\n","/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/Nav.jsx",[],["76","77"],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/board/Board.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/board/PetCard.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/petDetail/PetDetail.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/comments/Comments.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/comments/SingleComment.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/comments/CommentForm.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/Map.jsx",["78","79"],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/Myinfo.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/Signup.jsx",["80"],"// import React from \"react\";\n// import \"../styles/login.css\";\n\nimport axios from \"axios\";\nimport React, { Component } from \"react\";\nimport \"../styles/Signup.css\";\nimport { withRouter } from \"react-router-dom\";\n\nclass Signup extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      email: \"\",\n      password: \"\",\n      username: \"\",\n      mobile: \"\",\n    };\n  }\n\n  handleInputValue = (key) => (e) => {\n    this.setState({ [key]: e.target.value });\n  };\n\n  clickBtn = () => {\n    const { email, password, username, mobile } = this.state;\n\n    if (\n      password.length >= 4 &&\n      mobile[0] + mobile[1] + mobile[2] === \"010\" &&\n      mobile.length === 11 &&\n      email.includes(\"@\") &&\n      email.includes(\".\")\n    ) {\n      axios\n        .post(\"http://localhost:8080/auth/signup\", this.state)\n        .then((res) => {\n          this.props.history.push(\"/\");\n        })\n        .catch((err) => {\n          if (err) {\n            alert(\"이미 동일한 이메일이 존재합니다\");\n          }\n        });\n    } else {\n      alert(\"유효한 정보가 아닙니다. 다시 작성해주세요\");\n    }\n  };\n\n  render() {\n    return (\n      <div className=\"registerWrapper\">\n        <div className=\"registerName\"> Missing Animal </div>\n        <p className=\"registerText\">회원정보를 입력해주세요</p>\n        <input\n          placeholder=\"아이디(이메일)\"\n          type=\"email\"\n          className=\"inputEmail\"\n          onChange={this.handleInputValue(\"email\")}\n        />\n        <input\n          placeholder=\"비밀번호\"\n          type=\"password\"\n          className=\"inputPassword\"\n          onChange={this.handleInputValue(\"password\")}\n        />\n        <input\n          placeholder=\"이름(닉네임))\"\n          type=\"username\"\n          className=\"inputUsername\"\n          onChange={this.handleInputValue(\"username\")}\n        />\n        <input\n          placeholder=\"휴대폰 번호(숫자)\"\n          type=\"mobile\"\n          className=\"inputMobile\"\n          onChange={this.handleInputValue(\"mobile\")}\n        />\n        <button className=\"registerBtn\" onClick={() => this.clickBtn()}>\n          회원 가입하기\n        </button>\n      </div>\n    );\n  }\n}\n\nexport default withRouter(Signup);\n\n// function SignUp() {\n//   return <div className=\"Myinfo_menue\">회원가입</div>;\n// }\n// export default SignUp;\n","/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/Main_Menu.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/petRegister/PetRegister.jsx",[],"/Users/kimminguk/Desktop/fproject/Missing_Animal_Client/src/components/Signin.jsx",["81"],"// import React, { Component } from \"react\";\nimport React from \"react\";\nimport axios from \"axios\";\nimport \"../styles/Signin.css\";\n\nimport { withRouter, Link } from \"react-router-dom\";\n\nclass Signin extends React.Component {\n  constructor(props) {\n    super(props);\n\n    this.state = {\n      isLogin: false,\n      email: \"\",\n      password: \"\",\n      isEmailChecked: false,\n      isPasswordChecked: false,\n    };\n\n    this.loginHandler = this.loginHandler.bind(this);\n    this.getAccessToken = this.getAccessToken.bind(this);\n    this.naverLoginHandler = this.naverLoginHandler.bind(this);\n    this.kakaoLoginHandler = this.kakaoLoginHandler.bind(this);\n\n    this.NAVER_LOGIN_URL =\n      \"https://nid.naver.com/oauth2.0/authorize?response_type=code&client_id=vbUF8EMae9G5PmUfbyRh&state=MiAn&redirect_uri=http://localhost:3000/\";\n\n    this.KAKAO_LOGIN_URL =\n      \"https://kauth.kakao.com/oauth/authorize?client_id=e728a9738a0f6dd292c373b3ec9e5b45&redirect_uri=http://localhost:3000/&response_type=code\";\n  }\n\n  loginHandler() {\n    // this.setState({ isLogin: true });\n    window.localStorage.setItem(\"isLogin\", true);\n    this.props.close();\n    this.props.history.push({\n      // pathname: \"/\",\n      isLogin: this.state.isLogin,\n    });\n  }\n\n  async getAccessToken(authorizationCode) {\n    // 네이버\n    if (authorizationCode.length === 18) {\n      const resp = await axios.post(\n        \"http://localhost:8080/auth/naver\",\n        {\n          authorizationCode: authorizationCode,\n        },\n        { withCredentials: true }\n      );\n\n      if (resp.data.message === \"naver login\") {\n        console.log(\"userid:\", resp.data.userid);\n        console.log(\"username:\", resp.data.username);\n        this.props.history.push(\"/\");\n      }\n    }\n    // 카카오\n    else {\n      const resp = await axios.post(\n        \"http://localhost:8080/auth/kakao\",\n        {\n          authorizationCode: authorizationCode,\n        },\n        { withCredentials: true }\n      );\n\n      console.log(\"resp.data:\", resp.data);\n      console.log(\"resp.data.message:\", resp.data.message);\n\n      if (resp.data.message === \"kakao login\") {\n        this.props.history.push(\"/\");\n      }\n    }\n  }\n\n  componentDidMount() {\n    const url = new URL(window.location.href);\n    const authorizationCode = url.searchParams.get(\"code\");\n    if (authorizationCode) {\n      this.getAccessToken(authorizationCode);\n    }\n\n    console.log(\"authorizationCode:\", authorizationCode);\n  }\n\n  naverLoginHandler() {\n    window.location.assign(this.NAVER_LOGIN_URL);\n  }\n\n  kakaoLoginHandler() {\n    window.location.assign(this.KAKAO_LOGIN_URL);\n  }\n\n  emailValidator = (e) => {\n    const emailInput = e.target.value;\n    if (\n      emailInput.length > 4 &&\n      emailInput.includes(\"@\") &&\n      emailInput.includes(\".\")\n    ) {\n      this.setState({ isEmailChecked: true });\n      this.setState({ email: emailInput });\n    } else {\n      this.setState({ isEmailChecked: false });\n    }\n  };\n\n  passwordValidator = (e) => {\n    const passwordInput = e.target.value;\n    if (passwordInput.length >= 4) {\n      this.setState({ isPasswordChecked: true });\n      this.setState({ password: passwordInput });\n    } else {\n      this.setState({ isPasswordChecked: false });\n    }\n  };\n\n  loginRequestHandler = async () => {\n    const { email, password, isEmailChecked, isPasswordChecked } = this.state;\n\n    // 이메일, 비밀번호 체크\n    if (isEmailChecked === false || isPasswordChecked === false) {\n      alert(\"아이디, 비밀번호를 확인해주세요\");\n    } else if (isEmailChecked && isPasswordChecked) {\n      const loginRequest = await axios.post(\n        \"http://localhost:8080/auth/signin\",\n        { email, password },\n        { withCredentials: true }\n      );\n\n      if (loginRequest.data.message === \"signin OK\") {\n        console.log(\"isLogin:\", this.state.isLogin);\n        console.log(\"loginRequest.data:\", loginRequest.data);\n        this.loginHandler();\n      }\n    }\n  };\n\n  convertToSignup = () => {\n    this.props.history.push(\"/signup\");\n  };\n\n  render() {\n    const { isLogin } = this.state;\n    const { isOpen, close } = this.props;\n\n    // { isLogin ? <Board /> : <Signin loginHandler={this.loginHandler} />}\n    console.log(\"isOpen:\", isOpen);\n\n    return (\n      <>\n        {isOpen ? (\n          // isopen이 true: 코드를 실행, false: null\n          // <div onClick={close}> 버튼, 입력 창 제외 다른 부분(회색바탕) 클릭시 모달이 꺼짐\n          // <span className=\"close\" onClick={close}>&times;</span> (&times; = x버튼) 누를시 꺼짐\n          // isOpen이 true: 모달 켜짐, false: 모달 꺼짐\n\n          <div className=\"loginModal\">\n            {/* <div onClick={close}> */}\n            <div className=\"loginList\">\n              <span className=\"close\" onClick={close}>\n                &times;\n              </span>\n              <div className=\"modalContents\" onClick={() => isOpen}>\n                <div className=\"signinName\"> Missing Animal </div>\n                <input\n                  // name=\"email\"\n                  className=\"loginId\"\n                  // type=\"text\"\n                  placeholder=\"아이디(이메일)\"\n                  onChange={this.emailValidator.bind(this)}\n                />\n                <input\n                  // name=\"password\"\n                  className=\"loginPw\"\n                  type=\"password\"\n                  placeholder=\"비밀번호\"\n                  onChange={this.passwordValidator.bind(this)}\n                />\n                <button\n                  className=\"loginBtn\"\n                  onClick={this.loginRequestHandler.bind(this)}\n                >\n                  로그인\n                </button>\n                <div className=\"socialBox\">\n                  <div className=\"kakao\">\n                    <div\n                      className=\"btn__kakao\"\n                      onClick={this.kakaoLoginHandler}\n                    >\n                      카카오 계정으로 로그인\n                    </div>\n                  </div>\n                  <div className=\"naver\">\n                    <div\n                      className=\"btn__naver\"\n                      onClick={this.naverLoginHandler}\n                    >\n                      네이버 계정으로 로그인\n                    </div>\n                  </div>\n                </div>\n                <div>\n                  <Link to=\"/signup\" className=\"loginEnd\">\n                    <button\n                      className=\"signUpBtn\"\n                      onClick={this.convertToSignup.bind(this)}\n                    >\n                      회원가입\n                    </button>\n                  </Link>\n                </div>\n              </div>\n            </div>\n            {/* </div> */}\n          </div>\n        ) : null}\n      </>\n    );\n  }\n}\n\nexport default withRouter(Signin);\n",{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","severity":1,"message":"87","line":28,"column":17,"nodeType":"88","endLine":28,"endColumn":30},{"ruleId":"89","severity":1,"message":"90","line":8,"column":3,"nodeType":"91","messageId":"92","endLine":13,"endColumn":4},{"ruleId":"82","replacedBy":"93"},{"ruleId":"84","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":79,"column":18,"nodeType":"97","messageId":"98","endLine":79,"endColumn":29},{"ruleId":"95","severity":1,"message":"99","line":84,"column":18,"nodeType":"97","messageId":"98","endLine":84,"endColumn":29},{"ruleId":"95","severity":1,"message":"100","line":25,"column":30,"nodeType":"97","messageId":"98","endLine":25,"endColumn":38},{"ruleId":"95","severity":1,"message":"101","line":146,"column":13,"nodeType":"97","messageId":"98","endLine":146,"endColumn":20},"no-native-reassign",["102"],"no-negated-in-lhs",["103"],"react/jsx-pascal-case","Imported JSX component Main_Menu must be in PascalCase or SCREAMING_SNAKE_CASE","JSXOpeningElement","no-useless-constructor","Useless constructor.","MethodDefinition","noUselessConstructor",["102"],["103"],"no-unused-vars","'showMarkers' is defined but never used.","Identifier","unusedVar","'hideMarkers' is defined but never used.","'username' is assigned a value but never used.","'isLogin' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]